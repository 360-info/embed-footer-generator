    "@smui/textfield": "^6.0.0-beta.14",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sanitize-html": "^2.7.0",
    "smui-theme": "^6.0.0-beta.14",
    "svelte": "^3.44.0"
